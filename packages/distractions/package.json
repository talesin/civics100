{
  "name": "distractions",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist/ node_modules/ build/ coverage/",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "build": "npx tsup src/index.ts --dts --format esm --out-dir dist && npm run list-todos",
    "build:vercel": "npm run build",
    "package": "npm run lint && npm run test && npm run build",
    "test": "npx jest",
    "list-todos": "find src/ test/ -iname '*.ts' -exec grep --color -inHE '(TODO|FIXME|HACK)' '{}' \\;"
  },
  "dependencies": {
    "@effect/cli": "^0.63.4",
    "@effect/platform": "^0.84.3",
    "@effect/platform-node": "^0.83.3",
    "civics2json": "*",
    "effect": "^3.14.14",
    "sentence-similarity": "^3.0.0",
    "similarity-score": "^1.0.0",
    "ts-essentials": "^10.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
