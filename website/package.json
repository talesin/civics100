{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/ node_modules/ build/ coverage/ .next/  package-lock.json",
    "dev": "next dev --turbopack",
    "build": "npm run lint && npm run test && NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "export": "next build",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "list-todos": "find src/ test/ -iname '*.ts' -exec grep --color -inHE '(TODO|FIXME|HACK)' '{}' \\;",
    "vercel-build-local": "npm run clean && cd .. && npm install && cd website && npm run build",
    "vercel-simulate": "npm run clean && cd .. && npm ci && cd website && npm run lint && npm run test && NODE_ENV=production next build"
  },
  "dependencies": {
    "@effect/platform": "^0.88.0",
    "civics2json": "*",
    "effect": "^3.16.13",
    "next": "15.3.5",
    "questionnaire": "*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "20.19.8",
    "@types/react": "19.1.8",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
